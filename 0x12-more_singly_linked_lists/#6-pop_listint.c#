#include "lists.h"
/**
 * pop_listint - deletes the head node & returns the head node's data (n).
 * @head: pointer to head
 * Return: value of deleted node
 */
int pop_listint(listint_t **head)
{

	listint_t *nxtnode, *temp;
	int retval;

	if (*head == NULL)
		return (0);
	temp = *head;

	nxtnode = temp->next;
	retval = temp->n;
	free(temp);
	*head = nxtnode;

	return (retval);
}
